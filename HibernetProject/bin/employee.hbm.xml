<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
 <class name="com.example.EmployeeBean" table="emp555">
 <id column="empid" name="empid" type="int">
 <generator class="native"/>
 </id>
 <property column="empname" name="empname" type="java.lang.String" />
 <property column="empsalary" name="empsalary" type="java.lang.Integer" />
 </class>
</hibernate-mapping>